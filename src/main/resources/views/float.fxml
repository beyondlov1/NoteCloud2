<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <VBox>
         <children>
            <AnchorPane prefHeight="200.0" prefWidth="290.0" VBox.vgrow="SOMETIMES">
               <children>
                  <TextArea fx:id="contentTextAreaSave" onKeyReleased="#save" prefHeight="300.0" prefWidth="250.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
            <AnchorPane prefHeight="200.0" prefWidth="290.0" VBox.vgrow="SOMETIMES">
               <children>
                  <ListView fx:id="contentListView" prefHeight="200.0" prefWidth="250.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" />
               </children>
            </AnchorPane>
         </children>
      </VBox>
      <AnchorPane prefHeight="400.0" prefWidth="26.0">
         <Button fx:id="backToMainButton" defaultButton="true" layoutX="-28.0" layoutY="77.0" onAction="#backToMain" prefHeight="26.0" prefWidth="92.0" rotate="90.0" text="返回主页面" AnchorPane.leftAnchor="-28.0" AnchorPane.rightAnchor="-28.0" />
         <Button fx:id="exitButton" layoutX="-4.0" layoutY="309.0" onAction="#exit" rotate="90.0" text="退出" />
      </AnchorPane>
   </children>
</HBox>
